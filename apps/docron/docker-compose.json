{
    "services": [
        {
            "name": "docron",
            "image": "ghcr.io/primez/docron:2024.10.10.18",
            "internalPort": 8080,
            "isMain": true,
            "volumes": [
                {
                    "hostPath": "${APP_DATA_DIR}/data",
                    "containerPath": "data",
                    "readOnly": false
                },
                {
                    "hostPath": "/var/run/docker.sock",
                    "containerPath": "/var/run/docker.sock",
                    "readOnly": true
                }
            ],
            "environment": {
                "DockerConnection": "unix:///var/run/docker.sock",
                "DB": "Data Source=/data/docron.db;"
            }
        }
    ]
}